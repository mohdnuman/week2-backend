<link rel="stylesheet" href="<%=assetPath('/css/googlesigninbutton.css')%>">

<form action="/user/create" method="POST">
    <input type="text" name="name" placeholder="Full Name">
    <input type="email" name="email" placeholder="E-mail">
    <input type="password" name="password" placeholder="Password">
    <input type="password" name="confirm_password" placeholder="Confirm Password">
    <input type="submit" value="Sign Up">

</form>

<form action="/user/auth/google" method="get">

    <button class="googleSignIn" type="submit">
        <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/8399/G-on-white.svg" alt="Google logo">
        <span class="googleSignIn__text">Sign in with Google</span>
      </button>
</form>